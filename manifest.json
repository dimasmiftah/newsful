{
  "manifest_version": 3,
  "name": "Newsful",
  "version": "1",
  "author": "Dimas Mifthul Huda",
  "description": "Menghilangkan paginasi pada artikel berita.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.tribunnews.com/*",
        "*://*.kompas.com/*",
        "*://*.suara.com/*",
        "*://*.merdeka.com/*",
        "*://*.grid.id/*",
        "*://*.pikiran-rakyat.com/*",
        "*://*.viva.co.id/*"
      ],
      "js": ["script.js"]
    }
  ]
}
